	<tdsProxy xmlns="http://schemas.techsoftinc.com/TDSProxy/1.0">
		<listeners>
			<add name="IRBManager" listenOnPort="1433" forwardToHost="db3" forwardToPort="1433" sslCertStoreName="My" sslCertStoreLocation="CurrentUser" sslCertSubjectThumbprint="7e1bb6cc7aa2bdecb3d64c1a4c409337588b44ea" authenticatorDll="IRBManager.TDSProxy.Authenticator.dll" authenticatorClass="IRBManager.TDSProxy.Authenticator.Authenticator"/>
